doi = {10.1109/ICRA.2016.7487667},
file = {:C$\backslash$:/Users/danaukes/Documents/Mendeley Desktop/Mulgaonkar et al. - 2016 - The Flying Monkey A Mesoscale Robot That Can Run, Fly, And Grasp.pdf:pdf},
